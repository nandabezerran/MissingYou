            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".view.dialogs.PopUpAjudaEnviada"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".view.dialogs.PopUpAjudaRecebida"
